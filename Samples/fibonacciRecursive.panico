program fibonacciRecursive;

vars{
    result, num : int;
}

void calculateFibonacci(n:int){
    vars{
        temp1, temp2 : int;
    }
    if(n < 2){
        result = n;
    }else{
        temp1 = n-1;
        temp2 = n-2;
        calculateFibonacci(temp1);
        temp1 = result;
        calculateFibonacci(temp2);
        temp2 = result;
        result = temp1 + temp2;
    };
};

start{
    num = 30;
    calculateFibonacci(num);
    print("Fibonacci de ", num, " es: ", result);
}end
